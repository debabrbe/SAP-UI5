<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" 
		  controllerName="myfiori.controller.FirstView"
		  xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="My First Page">
		<headerContent>
			<Button press="onNext" icon="sap-icon://action" tooltip="Share" />
		</headerContent>
		<subHeader>
			<Toolbar>
				<SearchField id="idSrch" search="onSearch" liveChange="onSearch"/>
			</Toolbar>
		</subHeader>
		<footer>
			<Toolbar>
				<ToolbarSpacer></ToolbarSpacer>
				<Button icon="sap-icon://filter" tooltip="fruits filter" press="onFilter"></Button>
			</Toolbar>
		</footer>
		<List mode="SingleSelectMaster" id="idSpidy" items="{
									path: '/fruits',
									sorter: {
										path: 'type',
										group: true
									}
									}" itemPress="onItemPress"
									delete="onDelete"
									selectionChange="onItemPress">
			<ObjectListItem 
				title="{name}" intro="{type}" number="{price}" numberUnit="{currency}"
				icon="{Image}"
			></ObjectListItem>
